{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Local",
        "program": "${workspaceFolder}/src/index.ts",
        "preLaunchTask": "tsc: build - tsconfig.json",
        "env": {
          "ENVIRONMENT": "local"
        }
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Dev",
        "program": "${workspaceFolder}/src/index.ts",
        "preLaunchTask": "tsc: build - tsconfig.json",
        "env": {
          "ENVIRONMENT": "dev"
        }
      }
    ]
  }
  